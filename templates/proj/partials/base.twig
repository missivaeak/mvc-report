<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}RoadLike{% endblock %}</title>
        <link rel="icon" href="{{ asset('img/favicon.ico') }}">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('proj') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('proj') }}
        {% endblock %}
    </head>
    <body>
        {% block nav %}{{ include('proj/partials/nav.twig') }}{% endblock %}
        {% block header %}{{ include('proj/partials/header.twig') }}{% endblock %}
        <main class="main">
            {% block body %}{% endblock %}
        </main>
        {#{ include('partials/footer.html.twig') }#}
    </body>
</html>
